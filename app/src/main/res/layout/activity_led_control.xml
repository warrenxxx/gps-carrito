<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context="com.led.led.ledControl">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Robby Control"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Desconectar"
        android:id="@+id/button4"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/abajo"
        android:layout_alignStart="@+id/abajo"
        android:id="@+id/idlatitud" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/idlatitud"
        android:layout_alignStart="@+id/idlatitud"
        android:id="@+id/idlongitud"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignRight="@+id/idlatitud"
        android:layout_alignEnd="@+id/idlatitud" />

    <TextView
        android:text="Latitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_marginBottom="18dp"
        android:layout_above="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp" />

    <TextView
        android:text="Longitud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_marginBottom="15dp"
        android:layout_above="@+id/button4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <Button
        android:text="derecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/derecha"
        android:layout_below="@+id/arriba"
        android:layout_alignRight="@+id/tblatdest"
        android:layout_alignEnd="@+id/tblatdest"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp" />

    <Button
        android:text="ARRIBA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/arriba"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/tblatdest"
        android:layout_alignStart="@+id/tblatdest" />

    <TextView
        android:text="Latitud Destino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/idlatdes"
        android:layout_below="@+id/abajo"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        android:layout_marginTop="44dp" />

    <TextView
        android:text="Longitud Destino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/idlondes"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="abajo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/abajo"
        android:layout_below="@+id/izquierda"
        android:layout_toRightOf="@+id/button4"
        android:layout_toEndOf="@+id/button4" />

    <Button
        android:text="izquierda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/izquierda"
        android:layout_below="@+id/arriba"
        android:layout_toLeftOf="@+id/abajo"
        android:layout_toStartOf="@+id/abajo" />

    <Button
        android:text="RUN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/run"
        android:onClick="run"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/tblongdes"
        android:layout_below="@+id/idlatdes"
        android:layout_toRightOf="@+id/idlatitud"
        android:layout_toEndOf="@+id/idlatitud"
        android:text="-71.9574076" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/tblatdest"
        android:layout_alignBaseline="@+id/idlatdes"
        android:layout_alignBottom="@+id/idlatdes"
        android:layout_toRightOf="@+id/idlatitud"
        android:layout_toEndOf="@+id/idlatitud"
        android:text="-13.5239783" />

</RelativeLayout>
